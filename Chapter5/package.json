{
  "name": "umc_9th_nodejs",
  "version": "1.0.0",
  "type": "module",
  "description": "UMC 테스트 프로젝트",
  "main": "src/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node ./src/index.js",
    "dev": "nodemon -e js,json,prisma --exec \"prisma generate && node src/index.js\"",
    "prisma:generate": "dotenv -e .env -- prisma generate",
    "migrate:dev": "dotenv -e .env -- prisma migrate dev",
    "migrate:reset": "dotenv -e .env -- prisma migrate reset",
    "db:push": "dotenv -e .env -- prisma db push"
  },
